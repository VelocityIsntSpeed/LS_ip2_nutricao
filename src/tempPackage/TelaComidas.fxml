<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tempPackage.Test">
   <children>
      <HBox alignment="TOP_CENTER">
         <children>
            <VBox layoutX="10.0" layoutY="10.0">
               <children>
                  <Label text="Nome" wrapText="true" />
                  <TextField fx:id="txtFieldNome" />
               </children>
            </VBox>
            <VBox>
               <children>
                  <Label text="Valor Energético (kcal)" wrapText="true" />
                  <TextField fx:id="txtFieldValEnergetico" />
               </children>
            </VBox>
            <VBox layoutX="36.0" layoutY="10.0">
               <children>
                  <Label text="Proteína (g)" wrapText="true" />
                  <TextField fx:id="txtFieldProteina" />
               </children>
            </VBox>
            <VBox layoutX="159.0" layoutY="10.0">
               <children>
                  <Label text="Carboidratos (g)" wrapText="true" />
                  <TextField fx:id="txtFieldCarboidratos" />
               </children>
            </VBox>
            <Button mnemonicParsing="false" onAction="#btnAddComida" text="Adicionar Comida" />
         </children>
      </HBox>
      <ListView prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
